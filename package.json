{
  "name": "my-node-app",
  "version": "1.0.0",
  "description": "A Node.js project using Express.js, Knex.js and TypeScript",
  "main": "src/app.ts",
  "scripts": {
    "start": "node --inspect -r ts-node/register --env-file=.env src/app.ts",
    "migrate": "node --env-file=.env ./node_modules/knex/bin/cli.js migrate:latest",
    "rollback": "node --env-file=.env ./node_modules/knex/bin/cli.js migrate:rollback",
    "seed": "node --env-file=.env ./node_modules/knex/bin/cli.js seed:run",
    "knex": "knex",
    "lint": "eslint . --ext .ts",
    "test": "vitest run -u"
  },
  "keywords": [
    "node",
    "express",
    "knex",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "knex": "^2.5.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "pg": "^8.11.3",
    "ts-node": "^10.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
