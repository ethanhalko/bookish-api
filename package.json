{
  "name": "my-node-app",
  "version": "1.0.0",
  "description": "A Node.js project using Express.js, Knex.js and TypeScript",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node src/app.ts",
    "migrate": "node --env-file=.env ./node_modules/knex/bin/cli.js migrate:latest",
    "rollback": "node --env-file=.env ./node_modules/knex/bin/cli.js migrate:rollback",
    "seed": "knex seed:run",
    "migrate:make": "knex migrate:make"
  },
  "keywords": [
    "node",
    "express",
    "knex",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.95.11",
    "mysql": "^2.18.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.0"
  }
}